<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <engine:VisualElement name="ShipItem" data-source-type="TurtleUpgrade, Assembly-CSharp" style="flex-grow: initial; height: 80px; min-height: auto; flex-direction: row; align-items: stretch; justify-content: space-between; align-self: stretch; align-content: center; -unity-text-align: middle-left;">
        <engine:VisualElement name="Icon" style="flex-grow: initial; flex-basis: auto; width: 25%; flex-shrink: 1; flex-wrap: nowrap; -unity-background-scale-mode: scale-to-fit; background-size: cover; right: auto; align-items: auto; align-content: auto;">
            <Bindings>
                <engine:DataBinding property="style.backgroundImage" data-source-path="icon" binding-mode="ToTarget" />
            </Bindings>
        </engine:VisualElement>
        <engine:Label text="Name of the thing" name="Name" style="flex-shrink: 1; flex-grow: initial;">
            <Bindings>
                <engine:DataBinding property="text" data-source-path="type" binding-mode="ToTarget" source-to-ui-converters="shop" />
            </Bindings>
        </engine:Label>
        <engine:Label text="Level" name="Level">
            <Bindings>
                <engine:DataBinding property="text" data-source-path="level" binding-mode="ToTarget" source-to-ui-converters="shop" />
            </Bindings>
        </engine:Label>
        <engine:Label text="Price" name="Price">
            <Bindings>
                <engine:DataBinding property="text" data-source-path="price" binding-mode="ToTarget" />
            </Bindings>
        </engine:Label>
        <engine:Button text="Buy" name="Buy" />
    </engine:VisualElement>
</engine:UXML>
